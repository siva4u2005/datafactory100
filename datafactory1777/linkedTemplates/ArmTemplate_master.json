{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory1777"
		},
		"generaldatalakeacct7777_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'generaldatalakeacct7777'"
		},
		"synapseworkspace777_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'synapseworkspace777'"
		},
		"generaldatalakeacct7777_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://generaldatalakeacct7777.dfs.core.windows.net/"
		},
		"synapseworkspace777_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapseworkspace777.sql.azuresynapse.net"
		},
		"synapseworkspace777_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dedicatedsqlpool"
		},
		"synapseworkspace777_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory1777_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"generaldatalakeacct7777_accountKey": {
						"value": "[parameters('generaldatalakeacct7777_accountKey')]"
					},
					"synapseworkspace777_password": {
						"value": "[parameters('synapseworkspace777_password')]"
					},
					"generaldatalakeacct7777_properties_typeProperties_url": {
						"value": "[parameters('generaldatalakeacct7777_properties_typeProperties_url')]"
					},
					"synapseworkspace777_properties_typeProperties_server": {
						"value": "[parameters('synapseworkspace777_properties_typeProperties_server')]"
					},
					"synapseworkspace777_properties_typeProperties_database": {
						"value": "[parameters('synapseworkspace777_properties_typeProperties_database')]"
					},
					"synapseworkspace777_properties_typeProperties_userName": {
						"value": "[parameters('synapseworkspace777_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}